<Group labelreference="L111" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">no_zeros := proc(W0_M) 
#dado el factor <Font encoding="UTF-8">W0_M sin polos de la segunda factorizaci\303\263n obtenemos el factor W_M sin ceros en su determinante de la factorizaci\303\263n de Wiener-Hopf buscada</Font>
local n := RowDimension(W0_M);
local W_M := W0_M;
local a := solve(Determinant(W_M),z,dropmultiplicity=true);
      if (nops({a})&gt;1) then a:=a[1]
      else a;
      end if;
      
      if type(a, complex) and abs(a)&lt;1 then local V1 := convert(NullSpace(eval(W_M,z=a)), list); local V2 := convert(NullSpace(LinearAlgebra:-Transpose(Matrix(V1))), list); local V := Flatten([V1,V2]);
      local U := Matrix(simplify(Student:-LinearAlgebra:-GramSchmidt(V)));local Di:= IdentityMatrix(n,n); Di[1,1]:=(1-conjugate(a)*z)/(z-a);
      W_M := W0_M.U.Di;

      else W_M;
      end if;

end proc:</Text-field>
</Input>
</Group>
