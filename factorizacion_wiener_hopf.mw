<Group hide-input="false" labelreference="L105" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">no_polos := proc(W0) 
<Font encoding="UTF-8">#dado el factor W0 de primera factorizaci\303\263n (W = W_0.W_0^*) obtenemos el factor W0_M sin polos de la segunda factorizaci\303\263n</Font>
local n := RowDimension(W0);
local W0_M := W0;
local W1 := simplify(map(denom,W0));
  for local i from 1 to n do
    for local j from 1 to n do
      local p := solve(W1[i,j],z,dropmultiplicity=true);
      
      if (nops({p})&gt;1) then p:=p[1]
      else p;
      end if;
      
      if type(p, complex) and abs(p)&lt;1 then local U := IdentityMatrix(n,n); U[j,j]:=(z-p)/(1-conjugate(p)*z);
      W0_M := W0_M.U;

      else W0_M;
      end if; 
    
    end do
  end do;
end proc:</Text-field>
</Input>
<Input><Text-field prompt="&gt; " style="Maple Input" spaceabove="0" linebreak="space" rightmargin="0" linespacing="0.0" initial="0" firstindent="0" leftmargin="0" bulletsuffix="" alignment="left" bullet="none" spacebelow="0" pagebreak-before="false"></Text-field>
</Input>
</Group>
